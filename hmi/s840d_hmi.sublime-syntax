###############################################################################

  def-method-unload:
    - match: \b(?i:UNLOAD)\b
      scope: keyword.definition.method.unload.begin.s840d_hmi
      push: def-method-unload-body

  def-method-unload-body:
    - meta_content_scope: meta.block.method.unload.s840d_hmi
    - match: \b(?i:END_UNLOAD)\b
      scope: keyword.definition.method.unload.end.s840d_hmi
      pop: true
    - include: statements

###############################################################################
# OUTPUT block
#   second-level entity
#   exists in dialog and menu
#
#   OUTPUT(operand)
#   ....
#   END_CHANGE
###############################################################################

  def-method-output:
    - match: \b(?i:OUTPUT)\b
      scope: keyword.definition.method.output.begin.s840d_hmi
      push: def-method-output-argument

  def-method-output-argument:
    - match: \(
      scope: punctuation.section.parameters.begin.s840d_hmi
      set: def-method-output-argument-name
    - include: line-end

  def-method-output-argument-name:
    - meta_scope: meta.parameters.name.s840d_hmi
    - match: ','
      scope: punctuation.separator.s840d_hmi
      set: def-method-output-argument-version
    - include: operand
    - include: def-method-output-argument-end

  def-method-output-argument-version:
    - meta_content_scope: meta.parameters.version.s840d_hmi
    - include: number
    - include: def-method-output-argument-end

  def-method-output-argument-end:
    - match: \)|$
      scope: punctuation.section.parameters.end.s840d_hmi
      set: def-method-output-body
    - include: illegal

  def-method-output-body:
    - meta_content_scope: meta.block.method.output.s840d_hmi
    - match: \b(?i:END_OUTPUT)\b
      scope: keyword.definition.method.output.end.s840d_hmi
      pop: true
    # register not allowed in output
    - match: \b(?i:REG)\b
      scope: invalid.illegal.s840d_hmi
    - include: expressions

###############################################################################
# PRESS block
#   second-level entity
#   exists in dialog and menu
#   softkey pressed notification
#
#   PRESS(HSx)
#   ....
#   END_PRESS
###############################################################################

  def-method-press:
    - match: \b(?i:PRESS)\b
      scope: keyword.definition.method.press.begin.s840d_hmi
      push:
        - match: \(
          scope: punctuation.section.parameters.begin.s840d_hmi
          set:
            - meta_scope: meta.method.parameters.s840d_hmi
            - match: \)|$
              scope: punctuation.section.parameters.end.s840d_hmi
              set: def-method-press-body
            - match: '{{softkey}}'
              scope: entity.name.softkey.s840d_hmi
            - match: \b(?i:P[UD]|S[LRUD])\b
              scope: entity.name.key.s840d_hmi
            - include: illegal
        - include: line-end

  def-method-press-body:
    - meta_content_scope: meta.block.method.press.s840d_hmi
    - match: \b(?i:END_PRESS)\b
      scope: keyword.definition.method.press.end.s840d_hmi
      pop: true
    - include: statements

