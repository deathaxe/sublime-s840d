# OBJECT MEMBERS
###############################################################################

  operand-index:
    - match: \[
      scope: punctuation.section.item-access.begin.s840d_hmi
      set:
        - meta_scope: meta.item-access.s840d_hmi
        - match: \]
          scope: punctuation.section.item-access.end.s840d_hmi
          set: operand-members
        - include: axis-variables
        - include: expressions
    - include: operand-members

  operand-members:
    - match: \.
      scope: punctuation.accessor.parameters.s840d_hmi
      set:
        - meta_scope: meta.operand.attribute.s840d_hmi
        # list of valid attributes
        - match: \b(?i:a[cl]|bc(_[gsu]t)?|d[ot]|fc(_[gsu]t)?|fs|gt|hlp|l[it]|m(?:ax|in)|[su]t|t[gt]|typ|v(?:a[lr]|ld)|wr)\b
          scope: entity.other.attribute-name.s840d_hmi
          pop: true
        - include: illegal-pop
    - match: (?=\S)
      pop: true
