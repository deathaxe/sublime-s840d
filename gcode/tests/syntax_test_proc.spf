;            ^^^^^^^^^^^^^^^^^^^^^ meta.function.attributes - meta.function.name
; <- comment.blockno
;   ^ keyword.declaration.proc
;        ^ entity.name.function
;             ^^^^^ storage.modifier
;                   ^^^^^^ invalid.illegal
;                          ^^^^^^^ storage.modifier
;                                  ^ comment.line

; procedure attribute must finalize a block
N80 PROC name SBLOF (VAR BOOL DATA) ; comment
;   ^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^ meta.function
