%YAML 1.2
---
# https://www.sublimetext.com/docs/syntax.html
name: CMC Script
scope: source.upscr
version: 2

file_extensions:
  - upscr
  - upcfg
  - uplib

variables:
  # loating point values
  float: (?i:\d*(\.?)\d*EX[-+]?\d+|\d+(\.)\d*|(\.)\d+)

  builtin_functions: |-
    (?xi:
      FileExist
    | Exists
    | Version
    | Input
    | InputEnum
    | InputText
    | InputReal
    | InputInt
    | InputUInt
    | InputChoice
    | MathRound
    | Round
    | StringMatch
    | Match
    | DateTime
    | DOVar
    | StringReplace
    | StringPos
    | StringSubStr
    | StringLen
    | Msg
    | Error
    | Warning
    | ExtCall
    | Log
    | Logging
    | Patch
    | Prepare
    | Internal
    | TraceToFile
    | Write
    | restext
    | resfile
    | Copy
    | FileCopy
    | FileDelete
    | QueryIni
    | QueryXml
    | FileRead
    | FileWrite
    | Return
    | Skip
    | Redo
    | Abort
    )

contexts:
  main:
    - include: upscript

  upscript:
    - include: statements
    - include: expressions

  statements:
    - include: conditionals
    - include: loops
    - include: functions
    - include: sections
    - include: block-numbers

  expressions:
    - include: comments
    - include: directives
    - include: function-calls
    - include: groups
    - include: interpolations
    - include: variables
    - include: numbers
    - include: strings
    - include: events
    - include: literals
    - include: operators
    - include: illegal-stray

###[ COMMENTS ]################################################################

  comments:
    - match: ;
      scope: punctuation.definition.comment.upscr
      push: line-comment-semicolon-body

  line-comment-semicolon-body:
    - meta_scope: comment.line.semicolon.upscr
    # NOTE: if `\n` is used, reindent does not work
    # if a line is terminated with a comment.
    - match: $
      pop: 1

###[ PREPROCESSOR ]############################################################

  directives:
    - include: conditional-directives
    - include: include-directives
    - include: pragma-directives
    - include: rem-directives

  conditional-directives:
    - match: (#)\s*(?i:if)\b
      scope: keyword.control.directive.conditional.if.upscr
      captures:
        1: punctuation.definition.keyword.upscr
    - match: (#)\s*(?i:els?if)\b
      scope: keyword.control.directive.conditional.elseif.upscr
      captures:
        1: punctuation.definition.keyword.upscr
    - match: (#)\s*(?i:else)\b
      scope: keyword.control.directive.conditional.else.upscr
      captures:
        1: punctuation.definition.keyword.upscr
    - match: (#)\s*(?i:endif)\b
      scope: keyword.control.directive.conditional.end.upscr
      captures:
        1: punctuation.definition.keyword.upscr

  include-directives:
    - match: (#)\s*(?i:include)\b
      scope: keyword.control.directive.include.upscr
      captures:
        1: punctuation.definition.keyword.upscr
      push:
        - include-meta
        - include-value

  include-meta:
    - meta_include_prototype: false
    - meta_scope: meta.preprocessor.include.upscr
    - include: immediately-pop

  include-value:
    - meta_include_prototype: false
    - match: \"
      scope: punctuation.definition.string.begin.upscr
      set: include-value-body
    - include: eol-pop

  include-value-body:
    - meta_include_prototype: false
    - meta_scope: meta.string.upscr string.quoted.double.upscr
    - include: double-quoted-string-end
    - include: string-interpolations
    - match: \\
      scope: punctuation.separator.path.upscr
    # include paths support windows environment variables
    - match: (%)[^%]+(%)
      scope: variable.other.environment.upscr
      captures:
        1: punctuation.definition.variable.begin.upscr
        2: punctuation.definition.variable.end.upscr
      push:
        - clear_scopes: 1
        - meta_include_prototype: false
        - meta_scope: meta.interpolation.upscr
        - include: immediately-pop

  pragma-directives:
    - match: (#)\s*(?i:pragma)\b
      scope: keyword.control.directive.pragma.upscr
      captures:
        1: punctuation.definition.keyword.upscr
      push: pragma-value

  pragma-value:
    - meta_scope: meta.preprocessor.pragma.upscr
    - match: \w+\b
      scope: constant.other.upscr
    - include: eol-pop

  rem-directives:
    - match: (#)\s*(?i:rem)\b
      scope: keyword.control.directive.rem.upscr
      captures:
        1: punctuation.definition.keyword.upscr
      push: rem-value

  rem-value:
    - meta_scope: meta.preprocessor.rem.upscr
    - meta_content_scope: comment.line.rem.upscr
    - include: eol-pop

###[ SECTION DEFINITIONS ]#####################################################

  sections:
    - include: section-brackets
    - include: section-chandata

  section-chandata:
    - match: CHANDATA\b
      scope: meta.section.upscr entity.name.section.upscr
      push: section-argument-list

  section-argument-list:
    - match: \(
      scope: punctuation.section.arguments.begin.upscr
      set: section-argument-list-body
    - include: immediately-pop

  section-argument-list-body:
    - meta_scope: meta.section.arguments.upscr
    - match: \)
      scope: punctuation.section.arguments.begin.upscr
      pop: 1
    - include: expressions

  section-brackets:
    - match: \[
      scope: punctuation.section.brackets.begin.upscr
      push: section-bracket-body

  section-bracket-body:
    - meta_scope: meta.section.upscr meta.brackets.upscr
    - match: \]
      scope: punctuation.section.brackets.end.upscr
      pop: 1
    - match: '[[:alpha:]_]\w*\b'
      scope: entity.name.section.upscr
    - include: expressions

###[ FUNCTION DEFINITIONS ]####################################################

  functions:
    - match: (?i:func|proc)\b
      scope: meta.function.upscr keyword.declaration.function.upscr
      push:
        - function-block
        - function-parameter-list
        - function-name

  function-name:
    - match: '[[:alpha:]_][[:alnum:]_]+'
      scope: entity.name.function.upscr
      pop: 1
    - include: else-pop

  function-parameter-list:
    - meta_content_scope: meta.function.identifier.upscr
    - match: \(
      scope: punctuation.section.parameters.begin.upscr
      set: function-parameter-list-body
    - include: else-pop

  function-parameter-list-body:
    - clear_scopes: 1
    - meta_scope: meta.function.parameters.upscr
    - match: \)
      scope: punctuation.section.parameters.end.upscr
      pop: 1
    - match: ','
      scope: punctuation.separator.parameters.upscr
    - match: '\$'
      scope: variable.parameter.upscr

  function-block:
    - match: \{
      scope: punctuation.section.block.begin.upscr
      set: function-block-body
    - include: else-pop

  function-block-body:
    - meta_scope: meta.function.body.upscr meta.block.upscr
    - match: \}
      scope: punctuation.section.block.end.upscr
      pop: 1
    - include: upscript

###[ CONDITIONAL STATEMENTS ]##################################################

  conditionals:
    - match: (?i:if)\b
      scope: keyword.control.conditional.if.upscr
    - match: (?i:els?if)\b
      scope: keyword.control.conditional.if.upscr
    - match: (?i:else)\b
      scope: keyword.control.conditional.else.upscr
    - match: (?i:endif)\b
      scope: keyword.control.conditional.end.upscr

  loops:
    - match: (?i:while)\b
      scope: keyword.control.loop.while.upscr
    - match: (?i:endwhile)\b
      scope: keyword.control.loop.end.upscr

###[ GROUPS ]##################################################################

  groups:
    - match: \(
      scope: punctuation.section.group.begin.upscr
      push: group-body

  group-body:
    - meta_scope: meta.group.upscr
    - match: \)
      scope: punctuation.section.group.end.upscr
      pop: 1
    - include: expressions

###[ INTERPOLATIONS ]##########################################################

  interpolations:
    - match: (\$)\(
      scope: punctuation.section.interpolation.upscr
      captures:
        1: punctuation.definition.variable.upscr
      push: interpolation-body

  interpolation-body:
    - meta_scope: meta.interpolation.upscr
    - match: \)
      scope: punctuation.section.interpolation.end.upscr
      pop: 1
    - include: expressions

  string-interpolations:
    - match: (\$)\(
      scope: punctuation.section.interpolation.upscr
      captures:
        1: punctuation.definition.variable.upscr
      push: string-interpolation-body

  string-interpolation-body:
    - clear_scopes: 1
    - meta_scope: meta.interpolation.upscr
    - match: \)
      scope: punctuation.section.interpolation.end.upscr
      pop: 1
    - include: expressions

###[ FUNCTION CALLS ]##########################################################

  function-calls:
    - match: '{{builtin_functions}}\b'
      scope: meta.function-call.identifier.upscr support.function.upscr
      push: function-arguments-list
    - match: \w+(?=\()
      scope: meta.function-call.identifier.upscr variable.function.upscr
      push: function-arguments-list

  function-arguments-list:
    - match: \(
      scope: punctuation.section.arguments.begin.upscr
      push: function-arguments-list-body
    - include: immediately-pop

  function-arguments-list-body:
    - meta_scope: meta.function-call.arguments.upscr
    - match: \)
      scope: punctuation.section.arguments.end.upscr
      pop: 1
    - match: ','
      scope: punctuation.separator.arguments.upscr
    - include: upscript

###[ OPERATORS ]###############################################################

  operators:
    - match: '[-+*/&|:?]='
      scope: keyword.operator.assignment.augmented.upscr
    - match: <<
      scope: keyword.operator.concat.upscr
    - match: '[<>=!]=|<|>'
      scope: keyword.operator.comparison.upscr
    - match: '&&|\|\||!'
      scope: keyword.operator.logical.upscr
    - match: '[-+*/]'
      scope: keyword.operator.arithmetic.upscr
    - match: '[&|]'
      scope: keyword.operator.bitwise.upscr
    - match: '='
      scope: keyword.operator.assignment.upscr
    - match: \.
      scope: punctuation.accessor.dot.upscr
    - match: \,
      scope: punctuation.separator.comma.upscr

###[ LITERALS ]################################################################

  block-numbers:
    - match: ^\s*(N\d+\b)
      captures:
        1: comment.blockno.upscr

  events:
    - match: (?i:Event)\b
      scope: variable.language.event.upscr
    - match: (?i:OnInit|OnNext|OnEnd|OnProc)\b
      scope: variable.language.event.upscr

  literals:
    # builtin constants
    - match: (?i:false|true)\b
      scope: constant.language.boolean.upscr
    - match: (?i:null)\b
      scope: constant.language.null.upscr
    # file operation constants
    - match: (?i:RTS|ARC|NCU|PCU)\b
      scope: constant.language.filearea.upscr
    - match: '[A-Z_][A-Z0-9_]*\b'
      scope: constant.other.upscr

  numbers:
    # BiCo
    - match: (\d+)(:)(\d+)(\.)(\d+)
      scope: meta.bico.upscr
      captures:
        1: constant.numeric.value.driveobject.upscr   # drive object
        2: punctuation.accessor.colon.upscr
        3: constant.numeric.value.parameter.upscr     # parameter
        4: punctuation.accessor.dot.upscr
        5: constant.numeric.value.bit.upscr           # bit
    # Floats
    - match: '{{float}}'
      scope: meta.number.float.decimal.upscr constant.numeric.value.upscr
      captures:
        1: punctuation.separator.decimal.upscr
        2: punctuation.separator.decimal.upscr
        3: punctuation.separator.decimal.upscr
    # Integers
    - match: (0x)(\h*)\b
      scope: meta.number.integer.hexadecimal.upscr
      captures:
        1: constant.numeric.base.upscr
        2: constant.numeric.value.upscr
    - match: \d+\b
      scope: meta.number.integer.decimal.upscr constant.numeric.value.upscr
    # single quoted binary numbers
    - match: (')([Bb])
      captures:
        1: punctuation.definition.numeric.begin.upscr
        2: constant.numeric.base.upscr
      push: binary-number
    # single quoted hexadecimal numbers
    - match: (')([Hh])
      captures:
        1: punctuation.definition.numeric.begin.upscr
        2: constant.numeric.base.upscr
      push: hexadecimal-number

  binary-number:
    - meta_scope: meta.number.integer.binary.upscr
    - meta_content_scope: constant.numeric.value.upscr
    - match: \'|$
      scope: punctuation.definition.numeric.end.upscr
      pop: true
    - include: string-interpolations
    - match: '[^01]'
      scope: invalid.illegal.upscr

  hexadecimal-number:
    - meta_scope: meta.number.integer.hexadecimal.upscr
    - meta_content_scope: constant.numeric.value.upscr
    - match: \'|$
      scope: punctuation.definition.numeric.end.upscr
      pop: true
    - include: string-interpolations
    - match: \H
      scope: invalid.illegal.upscr

  strings:
    - include: double-quoted-strings
    - include: single-quoted-strings

  double-quoted-strings:
    - match: \"
      scope: punctuation.definition.string.begin.upscr
      push: double-quoted-string-body

  double-quoted-string-body:
    - meta_include_prototype: false
    - meta_scope: meta.string.upscr string.quoted.double.upscr
    - include: double-quoted-string-end
    - include: string-content

  double-quoted-string-end:
    - match: \"
      scope: punctuation.definition.string.end.upscr
      pop: 1
    - include: eol-pop

  single-quoted-strings:
    - match: \'
      scope: punctuation.definition.string.begin.upscr
      push: single-quoted-string-body

  single-quoted-string-body:
    - meta_include_prototype: false
    - meta_scope: meta.string.upscr string.quoted.single.upscr
    - include: single-quoted-string-end
    - include: string-content

  single-quoted-string-end:
    - match: \'
      scope: punctuation.definition.string.end.upscr
      pop: 1
    - include: eol-pop

  string-content:
    - include: escaped-chars
    - include: string-interpolations
    - include: scope:text.html.basic#tag
    - include: scope:text.html.basic#entities

  escaped-chars:
    - match: \\[afnrt]
      scope: constant.character.escape.upscr

###[ VARIABLES ]###############################################################

  variables:
    - include: section-qualifiers
    - include: drv-variables
    - include: nc-variables
    - include: up-variables

  section-qualifiers:
    - match: BD\b
      scope: meta.section.upscr variable.namespace.bd.upscr
      push: section-qualifier-name
    - match: NC\b
      scope: meta.section.upscr variable.namespace.nc.upscr
      push: section-qualifier-name
    - match: PS\b
      scope: meta.section.upscr variable.namespace.drive.upscr
      push: section-qualifier-name

  section-qualifier-name:
    - match: \[
      scope: punctuation.section.brackets.begin.upscr
      set: section-qualifier-name-body
    - include: immediately-pop

  section-qualifier-name-body:
    - meta_scope: meta.section.upscr meta.brackets.upscr
    - match: \]
      scope: punctuation.section.brackets.end.upscr
      pop: 1
    - match: '[[:alpha:]_]\w*\b'
      scope: entity.name.section.upscr
    - include: expressions

  drv-variables:
    - match: '[pr]\d+'
      scope: variable.language.parameter.upscr
      push: maybe-item-access

  nc-variables:
    # SINUMERIK PI constant
    # note: not evaluated by CMC
    - match: (\${1,2})(?i:PI)\b
      scope: support.variable.nck.pi.upscr
      captures:
        1: punctuation.definition.variable.nck.upscr
    # SINUMERIK synchron action R-Parameters
    # note: not evaluated by CMC
    - match: (\${0,2})(?i:RG?)\d+\b
      scope: support.variable.nck.r.upscr
      captures:
        1: punctuation.definition.variable.nck.upscr
    # SINUMERIK R-Parameter
    # note: not evaluated by CMC
    - match: (\${0,2})(?i:RG?)(?=[ \t]*\[)'
      scope: support.variable.nck.r.upscr
      captures:
        1: punctuation.definition.variable.nck.upscr
      push: maybe-item-access
    # SINUMERIK synchron action vars
    # note: not evaluated by CMC
    - match: (\${0,2})(?i:SYG_[ABCIRS][4-9MUS])\b
      scope: support.variable.nck.guds.upscr
      captures:
        1: punctuation.definition.variable.nck.upscr
      push: maybe-item-access
    # SINUMERIK nc variables
    # note: not evaluated by CMC
    - match: (\${1,2})(?:A(?:_(?:CMDSI|D(?:AY|B(?:[BDRW]|S[BW])|L[BDRW]|P(?:_(?:IN_(?:CONF|LENGTH|STATE|VALID)|OUT_(?:CONF|LENGTH|STATE|VALID))|B_(?:IN|OUT)|R_(?:IN|OUT)|S(?:B_(?:IN|OUT)|D_(?:IN|OUT)|W_(?:IN|OUT))|W_(?:IN|OUT)))|F(?:RDP_(?:ACK_REQ|DIAG|ERR(?:_REAC|OR)|S(?:ENDMODE|UB(?:S|S_ON)))|SDP_(?:DIAG|ERR(?:_REAC|OR)|SUBS_ON))|GG|HOUR|I(?:N|N(?:A|CO|S(?:[EI]|E(?:[DP]|PD)|I(?:[DP]|PD))))|L(?:EVELSID|INK_TRANS_RATE)|M(?:ARKERS(?:I|I(?:[DP]|PD))|INUTE|ON(?:IFACT|TH)|SECOND|YM(?:LN|N|T(?:LN|N)))|OU(?:T|T(?:A|S(?:[EI]|E(?:[DP]|PD)|I(?:[DP]|PD))))|P(?:B(?:B_(?:IN|OUT)|D_(?:IN|OUT)|R_(?:IN|OUT)|W_(?:IN|OUT))|LCSI(?:IN|OUT)|RO(?:B(?:E|E_LIMITED)|T(?:_LOCK|O|OC)))|S(?:ECOND|T(?:ATSID|OPESI))|T(?:IMERSI|OOLM(?:LN|N|T(?:LN|N)))|USED(?:[DT]|ND)|XFAULTSI|YEAR)|A_(?:A(?:C(?:C|C(?:_PERCENT|LIMA)|S_REL|T_INDEX_AX_POS_NO)|LARM_STAT|TOL|X(?:_DISABL(?:E|E_SRC)|CHANGE_(?:STAT|TYP)))|B(?:CS_OFFSET|RAKE_(?:COND[BM]|STATE))|C(?:HANNO|O(?:LLPOS|UP_(?:ACT|COR(?:R|R_DIST)|OFFS))|P(?:ACT(?:FA|LA)|BC|DEFLA|F(?:AC(?:CT|T)|CMD(?:PT|VT)|M(?:O[FN]|SON)|R(?:EQV|S))|L(?:ACC|C(?:MD[PV]|TID)|DEN|IN(?:SC|TR)|NUM|OUT(?:SC|TR)|S(?:ETVAL|TATE)|TYPE)|M(?:ALARM|RESET|STAR(?:T|TPRT)|VDI)|N(?:ACT(?:FA|LA)|DEFLA)|S(?:ETTYPE|YN(?:CO(?:[PV]|P2)|FI(?:[PV]|P2))))|URR)|D(?:E(?:LT|PAXO)|IAM_STAT|RF|T(?:B(?:[BW]|RE(?:[BM]|B_(?:C(?:MD|ORR)|DEP)|M_(?:C(?:MD|ORR)|DEP)))|E(?:[BW]|P[BW])|S[BW]))|E(?:G_(?:A(?:CTIVE|X)|DENOM|NUM(?:_LA|ERA)|SY(?:N|NFA)|TYPE)|NC(?:_(?:A(?:CTIVE|MPL)|COM(?:P|P_(?:IS_MODULO|M(?:AX|IN)|STEP)))|1_ACTIVE|2_ACTIVE)|SR_(?:ENABLE|STAT|TRIGGER)|TRANS)|F(?:GR(?:EF|OUP)|IX_POINT_SELECTED|OC|XS)|I(?:[BMW]|B(?:_CORR|[CN]|N_CORR)|E(?:N|N_CORR)|N(?:_SYNC|POS_STAT)|PO_NC_CHANAX|TR|W_CORR)|J(?:ERK(?:_(?:COUNT|T(?:IME|OT))|LIMA)|OG_POS_(?:ACT|SELECTED))|L(?:EAD_(?:[PV]|P_TURN|S[PV])|OA(?:D|D_SMOOTH))|M(?:A(?:CHAX|SL_(?:DEF|STAT))|EA(?:ACT|S_(?:P(?:1_VALID|2_VALID|3_VALID|4_VALID|OINT[1-4])|S(?:ET(?:ANGLE|POINT)|P_VALID)))|[MW]|M[1-4]|OTEND|W[1-4])|O(?:F(?:F|F_(?:LIMIT|VAL))|N_FIX_POINT|SCILL_(?:BREAK_POS[